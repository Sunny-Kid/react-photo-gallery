{
  "name": "react-photo-gallery",
  "version": "1.0.0",
  "description": "A photo gallery based on React",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-Kid/react-photo-gallery.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --watch --config ./webpack.config.js --progress --color",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --progress",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors",
    "prettier": "prettier-eslint --write $PWD/'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "yarn build:prod",
    "deploy": "yarn prepare && gh-pages -d dist"
  },
  "keywords": [
    "React",
    "photo",
    "gallery"
  ],
  "author": "Hank Ren",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional",
      "cz"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn prettier",
      "git add -A"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.2.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-css-modules": "^5.2.6",
    "commitizen": "^4.0.3",
    "commitlint-config-cz": "^0.12.1",
    "core-js": "3",
    "cross-env": "^6.0.2",
    "css-loader": "^3.2.0",
    "cz-customizable": "^6.2.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^2.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "image-webpack-loader": "^6.0.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^0.8.0",
    "minimist": "^1.2.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-less": "^3.1.4",
    "postcss-loader": "^3.0.0",
    "preload-webpack-plugin": "^2.3.0",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^5.0.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
